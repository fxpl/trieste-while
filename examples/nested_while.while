fun main() {
	var b; var first; var second; var post;
	b := 0;
	first := 0;
	second := 0;
	while (b = 0) do {
		while (b = 2) do {
			first := 1;
			first := 2;
		};
		while (b = 3) do {
			second := 1;
			second := 2;
		};
	};
	post := 1;
	output b;
	output first;
	output second;
	output post;
	return 0;
}
